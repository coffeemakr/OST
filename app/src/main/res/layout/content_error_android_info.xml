<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical">

    <TextView
        style="@style/ErrorReportSubtitle"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/title_android_release" />

    <TextView
        android:id="@+id/androidRelease"
        tools:text="4.10.2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <TextView
        style="@style/ErrorReportSubtitle"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/title_android_sdk" />

    <TextView
        android:id="@+id/androidSDK"
        tools:text="10"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

</LinearLayout>