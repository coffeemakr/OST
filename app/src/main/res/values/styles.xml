        <item name="ic_tune_24dp_no_vector">@drawable/ic_tune_white_24dp_no_vector</item>
        <item name="ic_restore_24dp">@drawable/ic_restore_white_24dp</item>
        <item name="ic_color_lens_24dp_no_vector">@drawable/ic_color_lens_white_24dp_no_vector
        </item>
        <!--<item name="toolbarStyle">@style/LightToolBarStyle</item>-->
        <item name="toolbarTheme">@style/ThemeOverlay.AppCompat.Dark.ActionBar
        </item> <!-- AppBarOverlay -->
        <!-- @style/GreenDarkTheme.PopupOverlay -->
        <item name="toolbarPopupTheme">@style/ThemeOverlay.AppCompat.Dark</item>
        <item name="dividerColor">@android:color/white</item>
    </style>

    <style name="Divider">
        <item name="android:layout_margin">0dp</item>
        <item name="android:padding">0dp</item>
        <item name="android:minHeight">1px</item>
        <item name="android:maxHeight">1dp</item>
        <item name="android:background">?attr/dividerColor</item>
        <item name="android:alpha">0.12</item>
    </style>

    <style name="MyCardView" parent="CardView.Dark">
        <item name="android:padding">8dp</item>
        <item name="android:layout_marginLeft">8dp</item>
        <item name="android:layout_marginRight">8dp</item>
    </style>


    <style name="ErrorReportValue" parent="@style/TextAppearance.AppCompat.Body1"></style>


    <style name="ErrorReportSubtitle" parent="@style/TextAppearance.AppCompat.Subhead">
        <item name="android:layout_marginBottom">8dp</item>
        <item name="android:layout_marginTop">8dp</item>
    </style>

    <style name="ErrorReportTitle" parent="@style/TextAppearance.AppCompat.Title" />

    <style name="DefaultStopDotsView">
        <item name="circleWidth">10dp</item>
        <item name="lineWidth">16dp</item>
        <item name="lineColor">?attr/colorPrimary</item>
        <item name="circleColor">@android:color/white</item>
        <item name="android:minWidth">16dp</item>
    </style>

    <style name="HeadButton" parent="Widget.AppCompat.Button">
        <item name="android:minHeight">36dp</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="HeadButtonTheme" parent="ThemeOverlay.AppCompat">
        <item name="colorButtonNormal">?attr/colorButtonInPrimary</item>
    </style>

    <style name="GreenDarkTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="GreenDarkTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>
